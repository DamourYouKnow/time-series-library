{
  "name": "time-series-library",
  "version": "3.3.0",
  "description": "A JavaScript library for vector arithmetic",
  "main": "module/time-series-library.js",
  "types": "module/time-series-library.d.ts",
  "scripts": {
    "clean": "rm -rf dist/*",
    "build": "npm run lint && npm run test && npm run tsc && npx rollup -c && npm run uglify",
    "tsc": "npx tsc -p ./src",
    "uglify": "npx uglifyjs-folder dist --each --output dist",
    "lint": "./node_modules/.bin/eslint . --ext .js,.ts,.tsx",
    "lint-fix": "./node_modules/.bin/eslint . --fix --ext .js,.ts,.tsx",
    "test": "./node_modules/.bin/mocha -r ./node_modules/ts-node/register ./tests/test.ts",
    "prebuild": "npm run clean",
    "prepare": "npm run tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/StatCan/time-series-library.git"
  },
  "author": "Bailey D'Amour",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.6",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^5.16.0",
    "mocha": "^5.2.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^3.4.0",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2",
    "uglifyjs-folder": "^1.5.1"
  },
  "dependencies": {}
}
